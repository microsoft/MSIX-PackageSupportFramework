{
    "applications": [
        {
            "id": "PSNoScripts",
            "executable": "PowershellScriptTest.exe"
        },
		{
			"id": "PSOnlyStart",
            "executable": "PowershellScriptTest.exe",
			"startScript":
			{
				"scriptPath": "SayHello.ps1",
				"runOnce": false
			}
		},
		{
			"id": "PSBothStartingFirst",
            "executable": "PowershellScriptTest.exe",
			"startScript" :
			{
				"scriptPath": "SayHello.ps1",
				"runOnce": false
			},
			"endScript" :
			{
				"scriptPath": "SayHi.ps1",
				"runOnce": false
			}
		},
		{
			"id": "PSBothEndingFirst",
            "executable": "PowershellScriptTest.exe",
			"endScript" :
			{
				"scriptPath": "SayHi.ps1",
				"runOnce": false
			},
			"startScript" :
			{
				"scriptPath": "SayHello.ps1",
				"runOnce": false
			}
		},
		{
			"id" : "PSScriptWithArg",
			"executable": "PowershellScriptTest.exe",
			"startScript" :
			{
				"scriptPath" : "SayWithArgument.ps1",
				"scriptArguments": "Start",
				"runOnce": false
			}
		},
		{
			"id" : "PSScriptInVirtual",
			"executable": "PowershellScriptTest.exe",
			"startScript" :
			{
				"scriptPath" : "SayHello.ps1",
				"runInVirtualEnvironment": true,
				"runOnce": false
			}
		}
    ],
    "processes": [
        {
            "executable": "PowershellScriptTest*"
        },
        {
            "executable": ".*"
        }
    ]
}